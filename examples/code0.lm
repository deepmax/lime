var arr = [6.0, 2.0, 8.0, 4.0, 7.0]

var len = alen(arr)

for var i: i32 = 1; i < len; i = i + 1 {
    var x = arr[i]
    var j = i
    for ; j > 0 and arr[j - 1] > x;  {
        arr[j] = arr[j - 1]
        j = j - 1
    }
    arr[j] = x
}

for var i: i32 = 0; i < len; i = i + 1 {
    print(arr[i], "\n")
}
