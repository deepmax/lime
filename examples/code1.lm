func factorial(n : i32) : i32 {
    if n <= 1 {
        ret 1
    }
    ret factorial(n - 1) * n
}


func max(a: i32, b: i32): i32 {
    if a > b {
        ret a
    } else {
        ret b
    }
}

var a = factorial(3)

print(a)


