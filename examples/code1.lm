func factorial(n: i32): i32 {
    if n <= 1 {
        return 1
    }
    return factorial(n - 1) * n
}


func max(a: i32, b: i32): i32 {
    if a > b {
        return a
    } else {
        return b
    }
}

var a = max(factorial(3), factorial(4))

print(a)


